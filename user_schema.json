{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "birthdate": {
      "type": "string",
      "format": "date-time",
      "description": "The birthdate of the user"
    },
    "created_time": {
      "type": "string",
      "format": "date-time",
      "description": "The time when the user account was created"
    },
    "display_name": {
      "type": "string",
      "description": "The display name of the user"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "The email address of the user"
    },
    "hometown": {
      "type": "string",
      "description": "The hometown of the user"
    },
    "photo_url": {
      "type": "string",
      "format": "uri",
      "description": "The URL to the user's profile photo"
    },
    "uid": {
      "type": "string",
      "description": "Unique identifier for the user"
    }
  },
  "required": ["birthdate", "created_time", "display_name", "email", "uid"]
}
